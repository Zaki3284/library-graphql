type Query {
    allEtudiants: [Etudiant!]!
    etudiantById(id: ID!): Etudiant
}

type Mutation {
    createEtudiant(name: String!, email: String!): Etudiant!
    updateEtudiant(id: ID!, name: String!, email: String!): Etudiant!
    deleteEtudiant(id: ID!): Boolean!
}

type Etudiant {
    id: ID!
    name: String!
    email: String!
}